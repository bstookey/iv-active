{
  "name": "iv_active",
  "version": "1.0.0",
  "description": "IV Active WP Migration",
  "author": "Bradley Stookey",
  "license": "GPL-2.0-or-later",
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --stats-children --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev:log": "npm run development 2>&1 | tee build.log",
    "watch:log": "npm run watch 2>&1 | tee build.log",
    "prod:log": "npm run production 2>&1 | tee build.log"
  },
  "dependencies": {
    "@fontsource/montserrat": "^5.1.1",
    "@fontsource/open-sans": "^5.1.1",
    "@fontsource/poppins": "^5.1.1",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "bootstrap": "^5.3.3",
    "browser-sync": "^3.0.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "browserslist": "^1.7.7",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "laravel-mix": "^6.0.49",
    "laravel-mix-eslint": "^0.2.0",
    "owl.carousel": "^2.3.4",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "sass-math": "^1.0.0",
    "svg-spritemap-webpack-plugin": "^4.5.0",
    "svg-transform-loader": "^2.0.13",
    "webpack": "^5.65.0",
    "webpack-cli": "^6.0.1"
  }
}
